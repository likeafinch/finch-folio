	"prettier.packageManager": "yarn",
	"prettier.singleQuote": true,
	"propTypes.arrayLike": true,
	"editor.renderFinalNewline": false,
	"files.autoSaveDelay": 4000,
	"cSpell.words": ["renderprops"]
}
